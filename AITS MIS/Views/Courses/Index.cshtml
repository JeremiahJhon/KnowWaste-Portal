
@{
    Layout = "~/Views/Shared/Shared.cshtml";
}

<link href="~/Content/course.css" rel="stylesheet" />


@using System.Data;

<div class="c-layout-page">
    <div class="c-layout-breadcrumbs-1 c-fonts-uppercase c-fonts-bold">
        <div class="container">
            <div class="c-page-title c-pull-left">
                <h3 class="c-font-uppercase c-font-sbold">E-learning / Curriculum</h3>
            </div>
            <ul class="c-page-breadcrumbs c-theme-nav c-pull-right c-fonts-regular">
                <li>
                    <a href="#">Knowledge Transfer</a>
                </li>
                <li>/</li>
                <li class="c-state_active">E-learning</li>
            </ul>
        </div>
    </div>
    <div class="c-content-box c-size-md c-bg-white">
        <div class="container">
            @Html.Raw(ViewBag.Curriculum)
        </div>
    </div>
    <div class="c-content-box c-size-md c-bg-white">
        <div class="container">
            @Html.Raw(ViewBag.Body)
        </div>
    </div>
</div>